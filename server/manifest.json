{
  "manifest_version": 2,
  "version": "{{version}}",
  "name": "{{name}}",
  "short_name": "{{shortName}}",
  "description": "{{description}}",
  "default_locale": "se",
  "icons": [
		{
			"src": "/icons/android-chrome-192x192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "/icons/android-chrome-256x256.png",
			"sizes": "256x256",
			"type": "image/png"
		}
	],
  "start_url": "/",
  "display": "standalone",
  "background_color": "{{backgroundColor}}",
  "theme_color": "{{themeColor}}"
}
