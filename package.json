{
  "name": "Vecka",
  "description": "Alltid veta vilken vecka det Ã¤r",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "DEBUG=vecka.14islands.com:* nodemon ./bin/www",
    "dev": "npm-run-all --parallel devstart watch browser-sync",
    "browser-sync": "browser-sync start --logLevel info --proxy localhost:3050 --files './public/' './views/*.ejs' --port 3051",
    "build:js-prod": "browserify public/main.js -t babelify -g uglifyify | uglifyjs -c > public/main-compiled.js",
    "build:css-prod": "stylus --include-css -c -u autoprefixer-stylus public/style.styl -o public/style.css",
    "build": "npm-run-all --parallel build:js-prod -s build:css-prod",
    "watch:css": "stylus -w --include-css -m -u autoprefixer-stylus public/style.styl -o public/style.css",
    "watch:js": "watchify public/main.js -d -o 'exorcist public/main-compiled.js.map > public/main-compiled.js' -t babelify",
    "watch": "npm-run-all --parallel watch:*",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.11.0",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-es2015-classes": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.15.1",
    "browser-sync": "^2.17.5",
    "browserify-shim": "^3.8.12",
    "component-loader-js": "^3.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "exorcist": "^0.4.0",
    "express": "~4.13.4",
    "express-babelify-middleware": "^0.2.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.15.1",
    "morgan": "~1.7.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "serve-favicon": "~2.3.0",
    "staticify": "0.0.6",
    "stylus": "0.54.5",
    "uglifyify": "^3.0.4",
    "watchify": "^3.7.0"
  }
}
